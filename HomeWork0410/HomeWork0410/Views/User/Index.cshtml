<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Surname</th>
      <th scope="col">Age</th>
      <th scope="col">Country</th>
      <th scope="col">PhoneNumber</th>
    </tr>
  </thead>
  <tbody>
    <td>@ViewBag.Name</td>
    <td>@ViewBag.Surname</td>
    <td>@ViewBag.Age</td>
    <td>@ViewBag.Country</td>
    <td>@ViewBag.PhoneNumber</td>
  
  </tbody>
</table>
